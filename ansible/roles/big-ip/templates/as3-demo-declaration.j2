{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.10.0",
        "id": "123abc",
        "label": "{{ demo_tenant }}",
        "remark": "HTTPS with predictive-node pool",
        "updateMode": "selective",
        "demo": {
            "class": "Tenant",
            "Application_1": {
                "class": "Application",
                "template": "https",
                "serviceMain": {
                    "class": "Service_HTTPS",
                    "virtualAddresses": [
                        "{{ demo_virtual_address }}"
                    ],
                    "snat": "auto",
                    "pool": "web_pool",
                    "serverTLS": "webtls"
                },
                "web_pool": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": [
                                "{{ app_server_address }}"
                            ],
                            "shareNodes": true
                        }
                    ]
                },
                "webtls": {
                    "class": "TLS_Server",
                    "certificates": [
                        {
                            "certificate": "webcert"
                        }
                    ]
                },
                "webcert": {
                    "class": "Certificate",
                    "certificate": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----",
                    "privateKey": "-----BEGIN RSA PRIVATE KEY-----\\n-----END RSA PRIVATE KEY-----"
                }
            }
        }
    }
}